{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "@composables/*": [
        "./src/composables/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ],
      "@modules/*": [
        "./src/modules/*"
      ],
      "@workers/*": [
        "./src/workers/*"
      ],
      "@functional/*": [
        "./src/helpers/functional/*"
      ],
    },
    "types": [
      "unplugin-icons/types/vue",
      "@intlify/unplugin-plugin-vue-i18n/messages",
      "vite-plugin-vue-layouts/client",
      "vite-plugin-pwa/client"
    ],
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "./components.d.ts"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
